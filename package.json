{
  "name": "fitnesscalculator",
  "version": "1.1.0",
  "description": "Simple body mass index and body fat percentage calculator.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . FitnessCalculator --overwrite --asar --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"FitnessCalculator\"",    
    "package-linux": "electron-packager . FitnessCalculator --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alc2110/BodyCalculator.git"
  },
  "author": "Alex Chlabicz",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.6.0",
    "node-stream": "^1.7.0"
  },
  "devDependencies": {
    "electron": "^12.0.2",
    "electron-packager": "^15.2.0"
  },
  "bugs": {
    "url": "https://github.com/Alc2110/BodyCalculator/issues"
  },
  "homepage": "https://github.com/Alc2110/BodyCalculator#readme"
}
